window['%hammerhead%'].utils.removeInjectedScript();

window['%hammerhead%'].start({
    cookie                   : {{{cookie}}},
    sessionId                : '{{{sessionId}}}',
    serviceMsgUrl            : '{{{serviceMsgUrl}}}',
    transportWorkerUrl       : '{{{transportWorkerUrl}}}',
    isFirstPageLoad          : {{{isFirstPageLoad}}},
    referer                  : {{{referer}}},
    proxyPort                : '{{{proxyPort}}}',
    forceProxySrcForImage    : {{{forceProxySrcForImage}}},
    iframeTaskScriptTemplate : {{{iframeTaskScriptTemplate}}},
    allowMultipleWindows     : {{{allowMultipleWindows}}},
    isRecordMode             : {{{isRecordMode}}},
    windowId                 : '{{{windowId}}}',
    nativeAutomation         : {{{nativeAutomation}}},
    disableCrossDomain       : {{{disableCrossDomain}}}
}, window);

(function () {
    {{{payloadScript}}}
})();
